    font-weight:bold;
    color: #3499E0;
  }
}
.index-project-detail .index-text{
  font-size: 12px;
  color: #666666;
}

.index-project-detail .progress-whole {
  margin: 5px 0 0;
  width: 260px;
  height: 4px;
  border-radius: 1px;
}
.index-project-detail .progress-content {
    background-color: #43CB83;
}

// index banner
.index-banner{
    background: linear-gradient(rgba(254, 250, 250, 0.35), rgba(135, 134, 134, 0.32)), url('<%= asset_path("banner-1.jpg") %>');
    display: block;
    // background-attachment: fixed;
    background-repeat: no-repeat;
    background-position: center top;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    border-style: none;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 0;
    border-radius: 0;
    min-height: 380px;
    position: relative;
    color: #fff;
    .cta-button {
        padding-top: 30px;
        margin: 0 auto;
        width: 200px;
        padding-bottom: 30px;
    }
    .hero-item-wrapper {
        width: 100%;
        min-width: 1200px;
    }
    .course-cost {
        font-size: 40px;
    }
}

.hero-title {
    padding-top: 80px;
    padding-left: 30px;
    padding-right: 30px;
    h1 {
        font-size: 32px;
        font-weight: bold;
    }
    p {
        padding-top: 18px;
        font-size: 14px;
    }
}
// index banner

